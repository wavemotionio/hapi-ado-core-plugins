{
  "name": "hapi-ado-core-plugins",
  "version": "2.2.0",
  "description": "Hapi.js plugins to augment Azure DevOps (ADAL-protected) microservers.",
  "repository": {
    "type": "git",
    "url": "https://github.com/wavemotionio/hapi-ado-core-plugins.git"
  },
  "main": "index.js",
  "files": [
    "lib/"
  ],
  "scripts": {
    "build": "rimraf lib/azureDevops && tsc && babel src --out-dir lib --extensions \".ts,.tsx\"",
    "clean": "rimraf lib/azureDevops",
    "dev": "npm run build && cross-env NODE_ENV=dev node tests/server.js",
    "type-check": "tsc --noEmit",
    "semantic-release": "semantic-release",
    "test": "./node_modules/.bin/eslint lib/**.js"
  },
  "pre-commit": ["test"],
  "keywords": [
    "hapi",
    "plugin",
    "adal",
    "Active Directory Authentication Library",
    "azure-devops-node-api"
  ],
  "author": "wavemotionio",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "async": "^2.6.1",
    "azure-devops-node-api": "^7.0.0",
    "bassmaster": "^3.2.0",
    "boom": "^7.3.0",
    "handlebars": "^4.0.12",
    "hapi-auth-cookie": "^9.1.0",
    "hapi-swagger": "^9.3.2",
    "inert": "^5.1.2",
    "joi": "^14.3.1",
    "lodash": "^4.17.11",
    "vision": "^5.4.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-transform-regenerator": "^7.3.4",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-typescript": "^7.1.0",
    "@types/node": "^10.12.18",
    "cross-env": "^5.2.0",
    "eslint": "^5.15.1",
    "hapi": "^18.1.0",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.6.3",
    "semantic-release": "^15.13.3",
    "typescript": "^3.2.2"
  }
}
